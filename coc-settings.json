{
  "diagnostic.errorSign": "✖",
  "diagnostic.warningSign": "⚑",
  "diagnostic.infoSign": "⚑",
  "diagnostic.hintSign": "⚑",
  "suggest.noselect": false,
  "explorer.icon.enableVimDevicons": true,
"explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": true,
  "explorer.floating.border.enable": true,
  "explorer.keyMappings": {
    "<cr>": ["explandable?", "expand", "open"],
    "v": "open:vsplit"
  },
  "explorer.floating.border.chars": [
    "─",
    "│",
    "─",
    "│",
    "┌",
    "┐",
    "┘",
    "└"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "json",
    "javascriptreact"
  ],
  "tsserver.formatOnType": false,
  "cSpell.userWords": [
    "Aval",
    "Borderless",
    "Entypo",
    "Ionicons",
    "Ngomana",
    "Numberz",
    "Tengo",
    "neumorph",
    "reduxjs",
    "routebutton"
  ],
  //explorer

  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.file.hiddenRules": {
    "filenames": ["node_modules", ".git"]
  }
}
